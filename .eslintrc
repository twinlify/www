{
  "extends": [
    "standard",
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  "parser": "babel-eslint",
  "rules": {
    "no-debugger": "off",
    "no-console": "off",
    "react/prop-types": "off"
  },
  "globals": {
    "fetch": false,
    "grecaptcha": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "ignorePatterns": [
    "*.html"
  ]
}
